{
	"name": "david63/resetlogin",
	"type": "phpbb-extension",
	"description": "Adds an Admin option to reset a user's login attempts",
	"version": "2.1.0",
	"time": "2015-11-03",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=5.4,<7.1",
		"phpbb/phpbb": ">=3.2.0-RC1",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Reset login attempts",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0-RC1"
    	},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/reset_login_version_file.json"
    	}
	}
}
